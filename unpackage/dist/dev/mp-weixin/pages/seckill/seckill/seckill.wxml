<view><view class="status" style="{{'position:'+(headerPosition)+';'}}"></view><view class="header" style="{{'position:'+(headerPosition)+';'}}"><block wx:for="{{timeList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['changeList',['$0',index],[[['timeList','',index]]]]]]]}}" class="{{[tabIndex==index?'active':'part']}}" bindtap="__e"><view class="time">{{item.time}}</view><view class="state">{{item.state}}</view></view></block></view><view class="place"></view><view class="goods-list"><view class="product-list"><block wx:for="{{goodsList}}" wx:for-item="goods" wx:for-index="index" wx:key="index"><view class="product"><image mode="widthFix" src="{{goods.pro_sn}}" data-event-opts="{{[['tap',[['toGoods',['$0'],[[['goodsList','',index]]]]]]]}}" bindtap="__e"></image><view class="name title-one">{{goods.pro_name}}</view><view class="info"><view style="text-align:left;"><view class="secprice"><text style="font-size:26rpx;">秒杀价:￥</text>{{goods.sec_price}}</view><view class="price"><text>原价:￥</text>{{goods.pro_price}}</view></view><image class="cart" src="../../../static/img/tabBar/cart-on.png" mode data-event-opts="{{[['tap',[['joinCart',['$0'],[[['goodsList','',index]]]]]]]}}" bindtap="__e"></image></view></view></block></view><view class="loading-text">{{loadingText}}</view></view></view>